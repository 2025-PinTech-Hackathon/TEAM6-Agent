{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "sidePanel"
    ],
    "side_panel": {
        "default_path": "index.html"
    },
    "action": {
        "default_title": "Click to open panel",
        "default_icon": "icon.png"
    }
}